        {
          "_name": "slider06",
          "_sourceTheme": "scalem5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"slider06 mbr-embla scalem5\" group=\"Sliders & Galleries\" plugins=\"embla\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showTitle\" title=\"Title\" checked>\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked>\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"1\" max=\"10\" value=\"8\" step=\"1\" condition=\"autoplay\">\n        <input type=\"checkbox\" title=\"Loop\" name=\"loop\" checked>\n        <input type=\"checkbox\" title=\"Draggable\" name=\"draggable\" checked>\n        <input type=\"checkbox\" name=\"arrows\" title=\"Arrows\" checked>\n        <input type=\"checkbox\" name=\"arrowsMobile\" title=\"Mobile Arrows\" condition=\"arrows\" checked>\n        <input type=\"color\" title=\"Arrows Color\" value=\"#635D60\" name=\"arrowsColor\" condition=\"arrows\" selected>\n        <input type=\"color\" title=\"Active Color\" value=\"#f59dff\" name=\"active\" condition=\"arrows\" selected>\n        <header>Cards</header>\n        <input type=\"range\" name=\"gap\" title=\"Gap\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"checkbox\" name=\"showName\" title=\"Card Name\" checked>\n        <input type=\"checkbox\" name=\"showRole\" title=\"Card Role\" checked>\n        <input type=\"checkbox\" name=\"showTag\" title=\"Card Tag\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#000000\" name=\"borderTag\" condition=\"showTag\" selected>\n        <input type=\"checkbox\" name=\"showIcon\" title=\"Card Icon\">\n        <input type=\"color\" title=\"Tag Icon Color\" value=\"#000000\" name=\"icon\" condition=\"showIcon\" selected>\n        <input type=\"color\" title=\"Active Icon Color\" value=\"#FF5753\" name=\"activeIcon\" condition=\"showIcon\" selected>\n        <input type=\"checkbox\" name=\"showButtons\" title=\"Card Buttons\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#000000\" name=\"border\" condition=\"showImage\" selected>\n        <input type=\"color\" title=\"Card BG 1\" value=\"#ff9466\" name=\"card_1\" selected>\n        <input type=\"color\" title=\"Card BG 2\" value=\"#ff9466\" name=\"card_2\" selected>\n        <input type=\"color\" title=\"Card BG 3\" value=\"#ff9466\" name=\"card_3\" selected>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax required>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/scalem5/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"title-wrapper\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\">\n                        <a href=\"https://trendee.com.tw/category/85?from=campaign_2404grandopening\" class=\"text-primary\">鹹食圈：</a><br><a href=\"https://trendee.com.tw/category/85?from=campaign_2404grandopening\" class=\"text-primary\">帶你吃點巷弄美食!</a></h2>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"container-fluid\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"embla\" data-loop=\"{{loop}}\" data-auto-play=\"{{autoplay}}\" data-auto-play-interval=\"{{interval}}\" data-draggable=\"{{draggable}}\" data-skip-snaps=\"true\" data-align=\"center\" data-contain-scroll=\"trimSnaps\">\n                    <div class=\"embla__viewport\">\n                        <div class=\"embla__container\">\n                            \n                            <div class=\"embla__slide slider-image item active\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-img\">\n                                            <div class=\"image-wrapper\">\n                                                \n                                            <a href=\"https://trendee.com.tw/product/261836650001?from=campaign_2404grandopening\" target=\"false\"><img src=\"@PROJECT_PATH@/assets/images/mai-150042635-x400x400.webp\" alt=\"Mobirise\" data-slide-to=\"0\" data-bs-slide-to=\"0\"></a></div>\n                                            <div class=\"tag-wrapper\">\n                                                <p class=\"item-tag mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-tag\" mbr-if=\"showTag\">圈圈獨家<br></p>\n                                                <a href=\"#\" class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                                                    <span mbr-icon class=\"mbr-iconfont fa-linkedin-square fa mobi-mbri-success mobi-mbri\"></span>\n                                                </a>\n                                            </div>\n                                        </div>\n                                        <div class=\"item-content\">\n                                            <h4 class=\"item-name mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-name, .mbr-section-btn\" mbr-if=\"showName\"><b>\n                                                <a href=\"https://trendee.com.tw/product/261836650001?from=campaign_2404grandopening\" class=\"text-primary\">金門嗆蟹</a></b></h4>\n                                            <p class=\"item-role mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-role\" mbr-if=\"showRole\">金門蟹與58度高粱的碰撞<br>蟹肉熟成下飯果凍狀<br><br>✦獨家免運組✦<br> 500公克/瓶 2入組<br></p>\n                                            <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-white\" href=\"https://trendee.com.tw/product/261836650001?from=campaign_2404grandopening\" data-app-placeholder=\"Type Text\"><span class=\"mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn\"></span>折後$968</a></div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div><div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-img\">\n                                            <div class=\"image-wrapper\">\n                                                \n                                            <a href=\"https://trendee.com.tw/product/83175321001?from=campaign_2404grandopening\" target=\"false\"></a><a href=\"https://trendee.com.tw/category/291?from=campaign_2404grandopening\"><img src=\"@PROJECT_PATH@/assets/images/mai-160622410-x700x700-500x500.jpg\" alt data-slide-to=\"1\" data-bs-slide-to=\"1\"></a></div>\n                                            <div class=\"tag-wrapper\">\n                                                <p class=\"item-tag mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-tag\" mbr-if=\"showTag\">\n                                                    圈圈獨家</p>\n                                                <a href=\"#\" class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                                                    <span mbr-icon class=\"mbr-iconfont fa-linkedin-square fa mobi-mbri-success mobi-mbri\"></span>\n                                                </a>\n                                            </div>\n                                        </div>\n                                        <div class=\"item-content\">\n                                            <h4 class=\"item-name mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-name, .mbr-section-btn\" mbr-if=\"showName\"><b><a href=\"https://trendee.com.tw/category/291?from=campaign_2404grandopening\" class=\"text-primary\">有利脆皮豬</a></b></h4>\n                                            <p class=\"item-role mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-role\" mbr-if=\"showRole\">採用23種香料醃製<br>爽脆咬勁配飯超狂<br><br>✦獨家免運組✦<br> 300公克/條 3入組&nbsp;</p>\n                                            <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-white\" href=\"https://trendee.com.tw/product/83175321001?from=campaign_2404grandopening\" data-app-placeholder=\"Type Text\"><span class=\"mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn\"></span>折後$1046</a></div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                <div class=\"slide-content\">\n                                    <div class=\"item-wrapper\">\n                                        <div class=\"item-img\">\n                                            <div class=\"image-wrapper\">\n                                                \n                                            <a href=\"https://trendee.com.tw/product/50846741023?from=campaign_2404grandopening\" target=\"false\"></a><a href=\"https://trendee.com.tw/category/180?from=campaign_2404grandopening\" target=\"true\"><img src=\"@PROJECT_PATH@/assets/images/2-4-500x500.jpg\" alt></a></div>\n                                            <div class=\"tag-wrapper\">\n                                                <p class=\"item-tag mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-tag\" mbr-if=\"showTag\">圈圈獨家</p>\n                                                <a href=\"#\" class=\"icon-wrapper\" mbr-if=\"showIcon\">\n                                                    <span mbr-icon class=\"mbr-iconfont fa-linkedin-square fa mobi-mbri-success mobi-mbri\"></span>\n                                                </a>\n                                            </div>\n                                        </div>\n                                        <div class=\"item-content\">\n                                            <h4 class=\"item-name mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-name, .mbr-section-btn\" mbr-if=\"showName\"><a href=\"https://trendee.com.tw/category/180?from=campaign_2404grandopening\" class=\"text-primary\">協發行泡菜</a></h4>\n                                            <p class=\"item-role mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-role\" mbr-if=\"showRole\">經典下酒組合<br>白飯小偷在這裡<br><br>✦獨家免運組✦ <br>下酒小菜4入組&nbsp;</p>\n                                            <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-white\" href=\"https://trendee.com.tw/product/50846741023?from=campaign_2404grandopening\" data-app-placeholder=\"Type Text\"><span class=\"mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn\"></span>限量$899</a></div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            \n                            \n                            \n                        </div>\n                    </div>\n                    <button class=\"embla__button embla__button--prev\">\n                        <span class=\"mobi-mbri mobi-mbri-left mbr-iconfont mbr-iconfont-btn\" aria-hidden=\"true\"></span>\n                        <span class=\"sr-only visually-hidden visually-hidden visually-hidden\">Previous</span>\n                    </button>\n                    <button class=\"embla__button embla__button--next\">\n                        <span class=\"mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn\" aria-hidden=\"true\"></span>\n                        <span class=\"sr-only visually-hidden visually-hidden visually-hidden\">Next</span>\n                    </button>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "& when not (@fullScreen)": {
